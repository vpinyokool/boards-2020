<%
var homefeedData;
//
var demoData = public.content.homefeed._data.pins;
var krizzly7Data = public.content.krizzly7._data.pins;
var marialat530Data = public.content.homefeed._data.pins;
var moominmom3Data = public.content.homefeed._data.pins;
var shavonData = public.content.homefeed._data.pins;
var thenifyshopperData = public.content.homefeed._data.pins;

if (mode == "krizzly7") {
	homefeedData = krizzly7Data;
}

if (mode == "marialat530") {
	homefeedData = marialat530Data;
}

if (mode == "moominmom3") {
	homefeedData = moominmom3Data;
}

if (mode == "shavon") {
	homefeedData = shavonData;
}

if (mode == "thenifyshopper") {
	homefeedData = thenifyshopperData;
}
%>
<div class="masonry-layout ">
	<% for ( var i=0; i < homefeedData.length; i++ ) { %>


		<div data-closeup-id="<%= i+1 %>" class="item pin-grid pin-id-<%= i+1 %>">
			<div class="pin-img-wrapper">
				<img src="<%= homefeedData[i].img %>" />
			</div>
			<div class="pin-title">
				<span class="reaction-marker wanna-try _hidden"><span>Wanna try</span> 路 </span>
				<span class="reaction-marker tried-it _hidden"><span>Tried it</span> 路 </span>
				<span class="reaction-marker love-it _hidden"><span>Love it</span> 路 </span>
				<span class="reaction-marker maybe _hidden"><span>Maybe</span> 路 </span>
				<%= homefeedData[i].title %>
			</div>
			<div class="pin-note _hidden"></div>
		</div>

	<% } %>
</div>