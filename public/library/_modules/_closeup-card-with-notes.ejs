<%
var homefeedData = public.content.homefeed._data.pins;

var krizzyly7Data = public.content.homefeed._data.pins;
var marialat530Data = public.content.homefeed._data.pins;
var moominmom3Data = public.content.homefeed._data.pins;
var shavonData = public.content.homefeed._data.pins;
var thenifyshopperData = public.content.homefeed._data.pins;

if (mode == "krizzyly7") {
	homefeedData = krizzyly7Data;
}

if (mode == "marialat530") {
	homefeedData = marialat530Data;
}

if (mode == "moominmom3") {
	homefeedData = moominmom3Data;
}

if (mode == "shavon") {
	homefeedData = shavonData;
}

if (mode == "thenifyshopper") {
	homefeedData = thenifyshopperData;
}
%>

<% for ( var i=0; i < homefeedData.length; i++ ) { %>
	<div class="main-card-wrapper cu-<%= i+1 %>">
		<img src="/assets/images/<%= homefeedData[i].img %>.png" />
		<div class="cu-pin-title"><%= homefeedData[i].title %></div>
		<div class="cu-pin-description">Pin description goes here</div>
		<div class="cu-pin-actions-wrap">
			<div class="icon-button-star star-<%= i+1 %>" id="star-<%= i+1 %>"></div>
			<div class="main-card-button-wrap">
				<div class="button secondary">Visit</div>
				<div class="button primary">Save</div>
			</div>
			<div class="icon-button overflow"></div>
		</div>
	</div>

<div data-closeup-id = "<%= i+1 %>" class="reaction-card-wrapper cu-<%= i+1 %>">
<div class="note-trigger-wrap kb-trigger">
				<div class="note-to-self-trigger">Add private note to self</div>
			</div>
			<div class="cu-title _hidden">Note to self</div>
			<div class="private-note _hidden"><span></span></div>
			<div class="button-sml _hidden">Edit</div>
		</div>


<% } %>
